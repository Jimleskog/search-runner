{
  "name": "search-runner",
  "displayName": "Search Runner",
  "description": "Let you search a document in vscode as a fast command",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Jimleskog/search-runner.git"
  },
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "search-runner.searchdocument",
        "title": "Search Runner"
      }
    ],
    "keybindings": [
      {
        "command": "search-runner.searchdocument",
        "key": "ctrl+alt+D",
        "mac": "cmd+rightclick",
        "when": "editorTextFocus"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "search-runner.searchdocument",
          "group": "navigation"
        }
      ]
    },
    "configuration": {
      "title": "Search Runner",
      "properties": {
        "extension.searchTranslation.documentName": {
          "type": "string",
          "default": "lib/Language/translationsmap.dart",
          "description": "Specify the path to the Document you want to search, from the root of your project"
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "eslint": "^8.54.0",
    "typescript": "^5.3.2",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8"
  }
}